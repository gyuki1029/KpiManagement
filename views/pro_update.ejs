    <!-- side bar & contents -->
    <div class="container-fluid">
      <div class="row">
        <!-- side ver -->
        <nav class="col-sm-3 col-md-2 hidden-xs-down bg-faded sidebar">
          <ul class="nav nav-pills flex-column">
  
          <li id="side_navigator_proupdate"><a class="nav-link" href="#section0">更新フォーム</a></li>
  
</ul>
        </nav>

        <!-- contents -->
      <main class="col-sm-9 offset-sm-3 col-md-10 offset-md-2 pt-3">
          
          <h3 id="section0" class="sub-header">更新フォーム</h3>
<div class="container">
  <form id="pro_registration_form">
    
      <div class="form-group row">
      <label for="code" class="col-sm-3 col-form-label">分類コード</label>
      <div class="col-sm-9"><input name="code" type="text" class="form-control" id="code" value='<%= pro_info.code%>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="name" class="col-sm-3 col-form-label">成果物名称</label>
      <div class="col-sm-9"><input name="name" type="text" class="form-control" id="name" value='<%= pro_info.name%>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="ruby" class="col-sm-3 col-form-label">読み方</label>
      <div class="col-sm-9"><input name="ruby" type="text" class="form-control" id="ruby" value='<%= pro_info.ruby%>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="background" class="col-sm-3 col-form-label">背景</label>
      <div class="col-sm-9"><textarea rows="5" name="background" type="text" class="form-control" id="background"><%= pro_info.background%></textarea></div>
      </div>
    
      <div class="form-group row">
      <label for="abst_ja" class="col-sm-3 col-form-label">成果物概要（日本語）</label>
      <div class="col-sm-9"><textarea rows="5" name="abst_ja" type="text" class="form-control" id="abst_ja"><%= pro_info.abst_ja %></textarea></div>
      </div>
    
      <div class="form-group row">
      <label for="abst_en" class="col-sm-3 col-form-label">成果物概要（英語）</label>
      <div class="col-sm-9"><textarea rows="5" name="abst_en" type="text" class="form-control" id="abst_en"><%= pro_info.abst_en %></textarea></div>
      </div>
    
      <div class="form-group row">
      <label for="status" class="col-sm-3 col-form-label">成果物ステータス</label>
      <div class="col-sm-9"><select name="status" class="form-control" id="status"><option>設計中</option><option>運用中</option><option>廃止</option></select></div>
      </div>
    
      <div class="form-group row">
      <label for="num_unit" class="col-sm-3 col-form-label">採番単位</label>
      <div class="col-sm-9"><input name="num_unit" type="text" class="form-control" id="num_unit" value='<%= pro_info.num_unit %>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="bus_func" class="col-sm-3 col-form-label">業務機能</label>
      <div class="col-sm-9"><input name="bus_func" type="text" class="form-control" id="bus_func" value='<%= pro_info.bus_func %>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="manage_dep" class="col-sm-3 col-form-label">管理部署</label>
      <div class="col-sm-9"><input name="manage_dep" type="text" class="form-control" id="manage_dep" value='<%= pro_info.manage_dep %>'></input></div>
      </div>
    
      <div class="form-group row">
      <label for="remarks" class="col-sm-3 col-form-label">備考</label>
      <div class="col-sm-9"><input name="remarks" type="text" class="form-control" id="remarks" value='<%= pro_info.remarks %>'></input></div>
      </div>
    

    <div class="form-group row">
      <div class="offset-sm-10 col-sm-2">
        <button type="button" class="btn btn-primary" id="pro_registration_button">更新</button>
      </div>
    </div>
  </form>
</div>
        </main>
      </div>
    </div>

